<h1>Listing Users</h1>

<table>
    <%= for user <- @users do %>
        <tr>
            <td><%= render "user.html", user: user %></td>
            <%= if user == @current_user do %>
                <td><%= link "List Videos", to: Routes.video_path(@conn, :index) %> </td>
            <% end %>
        </tr>
    <% end %>
</table>